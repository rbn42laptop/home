[lockdown]
disable-lock-screen=true

[wm/preferences]
theme='Ambiance-Flat-Teal'
titlebar-font='Lucida Gr 12'

[wm/keybindings]
close=['<Shift><Super>q']
switch-to-workspace-left=['<Primary><Alt><Super>h']
switch-to-workspace-right=['<Primary><Alt><Super>l']
switch-to-workspace-down=['<Primary><Alt><Super>j']
switch-to-workspace-up=['<Primary><Alt><Super>k']
move-to-workspace-right=['<Primary><Shift><Alt>l']
move-to-workspace-up=['<Primary><Shift><Alt>k']
move-to-workspace-down=['<Primary><Shift><Alt>j']
move-to-workspace-left=['<Primary><Shift><Alt>h']
switch-input-source=['disabled']
switch-input-source-backward=['disabled']

[interface]
font-name='Lucida Gr 12'
icon-theme='AwOkenWhite'
gtk-theme='Ambiance-Flat-Teal'

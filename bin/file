#!/bin/bash
source ~/bin/GLOBAL
#记录到rofi
python ~/bin/add2rofi.py "$ROFI_HISTORY" 1 ./bin/file "$@"
#nautilus 

export QML2_IMPORT_PATH=~/git/QMLProcess/build:~/git/qmltermwidget/build

export TERM=~/bin/term
export TERM=~/bin/term_blur

if [ $# -eq 0 ]
  then
    $TERM -e "ranger "
    #~/tmp/qmlterm/build/noblur -c ~/config/term/qmlterm/shadow_font_black.ini -e "ranger"
  else
    $TERM -e "ranger \"$@\""
    #~/tmp/qmlterm/build/noblur -c ~/config/term/qmlterm/shadow_font_black.ini -e "ranger \"$@\""
fi

